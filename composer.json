{
    "name": "qruz/qruz",
    "type": "project",
    "description": "Qruz Transportation Solution",
    "keywords": [
        "qruz",
        "transportation"
    ],
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-cors": "^0.11.2",
        "beyondcode/laravel-websockets": "^1.4",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "genealabs/laravel-sign-in-with-apple": "0.3.1",
        "laravel/framework": "^6.2",
        "laravel/horizon": "~3.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/vapor-cli": "^1.12",
        "laravel/vapor-core": "^2.10",
        "matthewbdaly/laravel-azure-storage": "^1.3",
        "mjaschen/phpgeo": "^3.0",
        "mll-lab/laravel-graphql-playground": "^2.0",
        "nuwave/lighthouse": "^4.8",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "rennokki/laravel-eloquent-query-cache": "^2.2",
        "tymon/jwt-auth": "dev-develop",
        "vinkla/hashids": "^7.0.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^3.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
